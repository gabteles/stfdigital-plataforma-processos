[
	{
		"header" : {
			"title": "Processo",
			"css": "text-center",
			"translate": "PESQUISA-AVANCADA-PROCESSOS.PROCESSO"
		},
		"result" : {
			"field": "result.classe + ' ' + result.numero",
			"css": "text-center"
		}
	},
	{
		"header" : {
			"title": "Protocolo",
			"css": "text-center",
			"translate": "PESQUISA-AVANCADA-PROCESSOS.PROTOCOLO"
		},
		"result" : {
			"field": "result.protocolo",
			"css": "text-center"
		}
	},
	{
		"header" : {
			"title": "Relator",
			"css": "text-center",
			"translate": "PESQUISA-AVANCADA-PROCESSOS.RELATOR"
		},
		"result" : {
			"field": "(result.distribuicoes.lenght > 0) ? result.distribuicoes[result.distribuicoes.lenght - 1].relator : ''"	
		}			
	},	
	{
		"header" : {
			"title": "Distribuição",
			"css": "text-center",
			"translate": "PESQUISA-AVANCADA-PROCESSOS.DISTRIBUICAO"
		},
		"result" : {
			"field": "(result.distribuicoes.lenght > 0) ? (result.distribuicoes[result.distribuicoes.lenght - 1].data * 1000 | date:'longDate') : ''",
			"css": "text-center"
		}			
	}   
]